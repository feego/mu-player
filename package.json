    "inquirer-menu": "^0.1.4",
    "inquirer-question": "^1.1.1",
    "intel": "^1.0.2",
    "komponist": "^0.1.0",
    "lastfmapi": "0.0.6",
    "lodash": "^3.10.1",
    "meow": "^3.4.1",
    "node-soundcloud": "0.0.4",
    "request": "^2.67.0",
    "split-tracklist": "^1.1.1",
    "update-notifier": "^0.5.0",
    "vk-universal-api": "0.0.2"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "mocha": "^2.3.3"
