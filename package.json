{
  "name": "mu-player",
  "version": "0.3.0",
  "description": "Play music from vk.com and soundcloud.com via Music Player Daemon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mink0/mu-player.git"
  },
  "author": "minkolazer",
  "main": "cli.js",
  "bin": {
    "mu": "cli.js"
  },
  "preferGlobal": "true",
  "scripts": {
